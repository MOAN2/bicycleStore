<view class="con">
  <u-back-top vue-id="2e637200-1" scroll-top="{{scrollTop}}" bind:__l="__l"></u-back-top>
  <view data-event-opts="{{[['tap',[['openPop',[true]]]]]}}" class="iconfont icon-fenlei  _i" bindtap="__e"></view>
  <top-custom vue-id="2e637200-2" title="{{pageTitle}}" show="{{show}}" bind:__l="__l"></top-custom>
  <list-item vue-id="2e637200-3" list="{{list}}" data-event-opts="{{[['^tap',[['setDetail']]]]}}" bind:tap="__e" bind:__l="__l"></list-item>
  <u-popup vue-id="2e637200-4" show="{{show}}" mode="left" data-event-opts="{{[['^close',[['closePop',[false]]]]]}}" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}">
    <view class="pop-cont">
      <u-collapse vue-id="{{('2e637200-5')+','+('2e637200-4')}}" bind:__l="__l" vue-slots="{{['default']}}">
        <u-collapse-item vue-id="{{('2e637200-6')+','+('2e637200-5')}}" title="按类型分类" name="type" bind:__l="__l" vue-slots="{{['default']}}">
          <block wx:for="{{typeList}}" wx:for-item="type" wx:for-index="index" wx:key="index">
            <u--text class="padding" vue-id="{{('2e637200-7-'+index)+','+('2e637200-6')}}" type="warning" text="{{type}}" data-event-opts="{{[['^tap',[['updateList',['$0'],[[['typeList','',index]]]]]]]}}" bind:tap="__e" bind:__l="__l"></u--text>
          </block>
        </u-collapse-item>
        <u-collapse-item vue-id="{{('2e637200-8')+','+('2e637200-5')}}" title="按年龄分类" name="sort" bind:__l="__l" vue-slots="{{['default']}}">
          <block wx:for="{{sortList}}" wx:for-item="sort" wx:for-index="index" wx:key="index">
            <u--text class="padding" vue-id="{{('2e637200-9-'+index)+','+('2e637200-8')}}" type="success" text="{{sort.title}}" data-event-opts="{{[['^tap',[['updateList',['$0'],[[['sortList','',index,'sortNum']]]]]]]}}" bind:tap="__e" bind:__l="__l"></u--text>
          </block>
        </u-collapse-item>
        <view class="allBcBox flex justify-around">
          <u--text class="padding u-border-bottom" vue-id="{{('2e637200-10')+','+('2e637200-5')}}" text="所有车辆" data-event-opts="{{[['^tap',[['getAllBicycle']]]]}}" bind:tap="__e" bind:__l="__l"></u--text>
          <view class="tagBox padding-top-sm padding-right-xs">
            <u-tag vue-id="{{('2e637200-11')+','+('2e637200-5')}}" text="{{allBcNum}}" plain="{{true}}" size="mini" type="warning" bind:__l="__l"></u-tag>
          </view>
        </view>
      </u-collapse>
    </view>
  </u-popup>
</view>